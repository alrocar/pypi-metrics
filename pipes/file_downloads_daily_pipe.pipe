VERSION 0

NODE file_downloads_mv
SQL >
    SELECT 
        toDate(timestamp) date,
        project,
        version,
        country_code,
        python,
        system_name,
        countState() downloads
    FROM file_downloads
    GROUP BY 
        date,
        project,
        version,
        country_code,
        python,
        system_name

TYPE materialized
DATASOURCE file_downloads_daily